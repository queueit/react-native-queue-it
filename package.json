{
  "name": "react-native-queue-it",
  "version": "1.2.0",
  "description": "React Native Module for integrating Queue-it's virtual waiting room into React Native apps.",
  "main": "lib/commonjs/index",
  "module": "lib/module/index",
  "types": "lib/typescript/index.d.ts",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "react-native-queue-it.podspec",
    "!lib/typescript/exampleApp",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.idea",
    "!**/build",
    "!**/.gradle",
    "!**/gradle*",
    "!**/local.properties",
    "!**/*.xcs*",
    "!**/*.plist",
    "!**/.settings"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepare": "bob build",
    "example": "yarn --cwd exampleApp",
    "pods": "cd exampleApp && pod-install --quiet",
    "bootstrap": "yarn exampleApp && yarn && yarn pods",
    "check-dependencies": "rnx-align-deps",
    "fix-dependencies": "rnx-align-deps --write"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/queueit/react-native-queue-it",
  "author": "Queue-it <dev@queue-it.com> (https://queue-it.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/queueit/react-native-queue-it/issues"
  },
  "homepage": "https://github.com/queueit/react-native-queue-it#readme",
  "devDependencies": {
    "@commitlint/config-conventional": "^8.3.4",
    "@react-native-community/bob": "0.17.1",
    "@react-native-community/eslint-config": "^3.0.1",
    "@react-native/codegen": "^0.80.1",
    "@react-native/gradle-plugin": "^0.80.1",
    "@release-it/conventional-changelog": "^1.1.4",
    "@rnx-kit/align-deps": "^3.0.8",
    "@types/jest": "^27.0.2",
    "@types/react": "^19.1.0",
    "babel-jest": "^29.7.0",
    "commitlint": "^8.3.5",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^4.3.8",
    "jest": "^29.7.0",
    "metro-react-native-babel-preset": "^0.77.0",
    "pod-install": "^0.1.28",
    "prettier": "^2.8.8",
    "react": "19.1.0",
    "release-it": "^13.7.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.6",
    "@react-native-community/checkbox": "^0.5.20",
    "@react-native-community/cli": "^18.0.0",
    "react-mixin": "^2.0.2",
    "react-native": "^0.80.1"
  },
  "peerDependencies": {
    "react-native": "^0.80.1"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/exampleApp/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn typescript"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "extends": [
      "prettier",
      "@react-native-community",
      "eslint:recommended",
      "plugin:jest/recommended"
    ],
    "rules": {
      "comma-dangle": "off",
      "no-unused-vars": "warn"
    },
    "env": {
      "jest/globals": true
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "@react-native-community/bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "codegenConfig": {
    "name": "ReactNativeQueueIt",
    "type": "modules",
    "jsSrcsDir": "./src/specs",
    "android": {
      "javaPackageName": "com.reactnativequeueit",
      "jniOutputDir": "./android/build/generated/source/codegen/jni"
    }
  },
  "rnx-kit": {
    "kitType": "library",
    "alignDeps": {
      "requirements": {
        "development": [
          "react-native@0.80.1"
        ],
        "production": [
          "react-native@*"
        ]
      },
      "capabilities": [
        "core",
        "core-android",
        "core-ios",
        "jest",
        "react"
      ]
    }
  }
}
